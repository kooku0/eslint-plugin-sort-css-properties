{
  "name": "@kooku0/eslint-plugin-sort-css-properties",
  "private": false,
  "version": "0.0.13",
  "description": "A custom ESLint plugin to sort CSS properties in specific order",
  "main": "./dist/cjs/index.js",
  "files": [
    "dist/**/*",
    "README.md"
  ],
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "require": "./dist/cjs/index.js",
      "default": "./dist/cjs/index.js"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kooku0/eslint-plugin-sort-css-properties.git"
  },
  "keywords": [
    "eslint",
    "eslint-plugin",
    "css",
    "sort"
  ],
  "scripts": {
    "build": "npm run build:cjs & npm run build:types",
    "build:cjs": "tsc --p ./tsconfig.cjs.json",
    "build:esm": "tsc --p ./tsconfig.esm.json",
    "build:types": "tsc --p ./tsconfig.types.json",
    "test": "jest --verbose",
    "release": "npm run build && npm version patch && npm publish"
  },
  "author": "kooku0 <koomg9599@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@types/eslint": "^8.56.10",
    "@types/lodash.camelcase": "^4.3.9",
    "@types/pascalcase": "^1.0.3",
    "@typescript-eslint/parser": "^5.0.0",
    "lodash.camelcase": "^4.3.0",
    "ts-node": "^10.9.2",
    "typescript": "~5.1.6",
    "@types/jest": "^29.0.0",
    "jest": "^29.0.0",
    "ts-jest": "^29.0.0"
  },
  "peerDependencies": {
    "eslint": ">=7"
  },
  "engines": {
    "node": ">= 18"
  },
  "dependencies": {
    "camelcase": "^8.0.0"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://npm.pkg.github.com"
  }
}
